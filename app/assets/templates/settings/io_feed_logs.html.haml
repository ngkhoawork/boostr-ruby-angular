#integration-logs.container-fluid.block.col-lg-10.col-lg-offset-1.col-md-12
  %div.mb20
    %span.title Io Feed Logs

  .table-report.pad
    %table.table.table-hover
      %thead
        %tr
          %th.left Date
          %th.center{width: '90px'} Processed
          %th.center{width: '90px'} Imported
          %th.center{width: '90px'} Failed
          %th.center{width: '90px'} Skipped
          %th.left{width: '150px'} Error Messages
          %th.left File Source
          %th.left Object Name

      %tbody
        %tr{'ng-repeat': 'log in logs' }
          %td {{log.created_at | date: 'medium' }}
          %td.center
            %b {{log.rows_processed}}
          %td.center
            %b {{log.rows_imported}}
          %td.center
            %b {{log.rows_failed}}
          %td.center
            %b {{log.rows_skipped}}
          %td
            %button.btn.activity-email-btn{'ng-click': 'showBodyModal(log.error_messages)'}
              Show Errors
          %td
            %b {{ log.file_source }}
          %td
            %b {{ log.object_name }}
