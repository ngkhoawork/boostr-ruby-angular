%tr{'ng-repeat': 'row in revenue | orderBy : zSort.by : zSort.rev | filter:searchText', 'ng-click': 'showAssignPmpAdvertiserModal(row)'}

  %td.left {{ row.name }}
  %td.left {{ row.ssp_advertiser.name }}
  %td.right {{ row.impressions | number:0 }}
  %td.right {{ row.revenue_loc | currency:row.currency.curr_symbol:2 }}
  %td.right
    %i.fa.fa-angle-right
